const STORAGE_KEY = "continue_watching";
class CarouselComponent {
    constructor(carouselService, breakpointObserver) {
        this.carouselService = carouselService;
        this.data_formatted = [];
        this.isMobile = false;
        breakpointObserver.observe([
            _angular_cdk_layout__WEBPACK_IMPORTED_MODULE_0__["Breakpoints"].HandsetLandscape,
            _angular_cdk_layout__WEBPACK_IMPORTED_MODULE_0__["Breakpoints"].HandsetPortrait
        ]).subscribe(result => {
            if (result.matches) {
                this.isMobile = true;
            }
        });
    }
    ngOnInit() {
        switch (this.carousel_type) {
            case "mix": {
                this.fetchMedia();
                break;
            }
            case "normal": {
                this.fetchImage();
                break;
            }
            case "recommendation": {
                this.fetchRecommendation();
                break;
            }
            case "similar": {
                this.fetchSimilar();
                break;
            }
        }
    }
    fetchImage() {
        let data = [];
        this.carouselService.getResponse(this.url).subscribe((res) => {
            data = res;
            data.forEach((element) => {
                if (this.media_type == "tv") {
                    element.title = element.name;
                }
            });
            this.carouselService.filterNullImage(data, this.data_formatted);
        });
    }
    fetchRecommendation() {
        this.carouselService.getRecommendation(this.media_id, this.media_type).subscribe((res) => {
            this.carouselService.filterNullImage(res, this.data_formatted);
        });
    }
    fetchSimilar() {
        this.carouselService.getSimilar(this.media_id, this.media_type).subscribe((res) => {
            this.carouselService.filterNullImage(res, this.data_formatted);
        });
    }
    fetchMedia() {
        let watch_list = JSON.parse(window.localStorage.getItem(STORAGE_KEY));
        if (watch_list == null) {
            this.data_formatted = null;
        }
        else {
            this.carouselService.filterNullImage(watch_list.reverse(), this.data_formatted);
        }
    }
    storeOnLocalStorage(media_type, media_id, poster_path, title) {
        let continue_watch_list = JSON.parse(window.localStorage.getItem(STORAGE_KEY)) || null;
        if (continue_watch_list == null) {
            continue_watch_list = [];
        }
        else if (!(continue_watch_list instanceof Array)) {
            continue_watch_list = [continue_watch_list];
        }
        const record = {
            "media_type": media_type,
            "id": media_id,
            "poster_path": poster_path,
            "title": title,
        };
        for (let i = 0; i < continue_watch_list.length; i++) {
            if (continue_watch_list[i].id == record.id) {
                continue_watch_list.splice(i, 1);
            }
        }
        continue_watch_list.push(record);
        window.localStorage.setItem(STORAGE_KEY, JSON.stringify(continue_watch_list));
    }
}
CarouselComponent.ɵfac = function CarouselComponent_Factory(t) { return new (t || CarouselComponent)(_angular_core__WEBPACK_IMPORTED_MODULE_1__["ɵɵdirectiveInject"](_services_carousel_service__WEBPACK_IMPORTED_MODULE_2__["CarouselService"]), _angular_core__WEBPACK_IMPORTED_MODULE_1__["ɵɵdirectiveInject"](_angular_cdk_layout__WEBPACK_IMPORTED_MODULE_0__["BreakpointObserver"])); };
