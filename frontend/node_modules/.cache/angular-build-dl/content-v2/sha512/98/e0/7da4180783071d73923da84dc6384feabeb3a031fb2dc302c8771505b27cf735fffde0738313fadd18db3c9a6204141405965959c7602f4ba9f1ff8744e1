class ReviewComponent {
    constructor(displayService, breakpointObserver) {
        this.displayService = displayService;
        this.isMobile = false;
        breakpointObserver.observe([
            _angular_cdk_layout__WEBPACK_IMPORTED_MODULE_0__["Breakpoints"].HandsetLandscape,
            _angular_cdk_layout__WEBPACK_IMPORTED_MODULE_0__["Breakpoints"].HandsetPortrait
        ]).subscribe(result => {
            if (result.matches) {
                this.isMobile = true;
            }
        });
    }
    ngOnInit() {
        this.getReviews();
    }
    getReviews() {
        this.displayService.getReview(this.media_id, this.media_type).subscribe((res) => {
            this.reviews = res;
            this.reviews.forEach((element) => {
                console.log("created_at", element.created_at);
                var options = { year: 'numeric', month: 'long', day: 'numeric', hour: '2-digit', minute: '2-digit', second: '2-digit', hour12: true };
                element.created_at = new Date(element.created_at).toLocaleString('en-US', options);
            });
            if (this.reviews.length >= 10) {
                this.reviews = this.reviews.slice(0, 10);
                this.counter = 10;
            }
            else {
                this.counter = this.reviews.length;
            }
        });
    }
}
