class CarouselService {
    constructor(httpClient) {
        this.httpClient = httpClient;
    }
    getResponse(url) {
        return this.httpClient.get(url);
    }
    getRecommendation(media_id, media_type) {
        let URL = "http://localhost:8080/" + media_type + "/recommendation?id=" + media_id;
        return new rxjs__WEBPACK_IMPORTED_MODULE_0__["Observable"]((observer) => {
            this.getResponse(URL).subscribe((res) => {
                observer.next(res);
            });
        });
    }
    ;
    getSimilar(media_id, media_type) {
        let URL = "http://localhost:8080/" + media_type + "/similar?id=" + media_id;
        return new rxjs__WEBPACK_IMPORTED_MODULE_0__["Observable"]((observer) => {
            this.getResponse(URL).subscribe((res) => {
                observer.next(res);
            });
        });
    }
    filterNullImage(data, data_formatted) {
        if (this.isMobile()) {
            (data || []).slice(0, 24).forEach((item) => {
                data_formatted.push(item);
            });
            console.log(data_formatted);
            return;
        }
        var j = -1;
        let length = (data.length > 24) ? 24 : data.length;
        for (var i = 0; i < length; i++) {
            if (i % 6 == 0) {
                j++;
                data_formatted[j] = [];
                data_formatted[j].push(data[i]);
            }
            else {
                data_formatted[j].push(data[i]);
            }
        }
        console.log("data-formatted", data_formatted);
    }
    isMobile() {
        if ((navigator.userAgent.match(/(phone|pad|pod|iPhone|iPod|ios|iPad|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i))) {
            return true;
        }
        else {
            return false;
        }
    }
}
