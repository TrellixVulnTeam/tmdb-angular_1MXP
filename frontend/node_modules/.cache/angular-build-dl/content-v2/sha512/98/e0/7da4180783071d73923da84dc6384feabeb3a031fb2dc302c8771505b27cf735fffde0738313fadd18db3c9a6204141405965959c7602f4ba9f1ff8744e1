class CastComponent {
    constructor(displayService, modalService) {
        this.displayService = displayService;
        this.modalService = modalService;
    }
    ngOnInit() {
        this.getCardArray();
    }
    open(person_id, person_img, content) {
        this.getPerson(person_id);
        this.getPersonExternalId(person_id);
        this.person_profile = person_img;
        this.modalService.open(content);
    }
    getCardArray() {
        this.displayService.getCast(this.media_id, this.media_type).subscribe((res) => {
            this.cast_formatted = res;
        });
    }
    getPerson(person_id) {
        this.displayService.getPersonDetails(person_id).subscribe((res) => {
            this.person = res;
        });
    }
    getPersonExternalId(person_id) {
        this.displayService.getPersonExternalIds(person_id).subscribe((res) => {
            this.person_ids = res;
            console.log("external id", this.person_ids);
        });
    }
}
